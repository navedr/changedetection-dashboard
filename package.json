{
    "name": "changedetection-dashboard",
    "version": "1.0.0",
    "description": "ChangeDetection Dashboard - A dashboard for ChangeDetection.io",
    "main": "src/index.ts",
    "scripts": {
        "dev": "npm-run-all --parallel dev:webpack dev:vite:server",
        "dev:vite:server": "vite-node --watch src/index.ts",
        "dev:webpack": "rimraf dist && webpack --mode development --watch",
        "lint:ci": "echo 'Not Implemented'",
        "prettier": "npx prettier --write \"**/*.{ts,tsx}\"",
        "build:ci": "npm-run-all --parallel build:webpack build:vite:server",
        "build:vite:server": "vite build --config vite.config.server.js",
        "build:webpack": "rimraf dist && webpack --mode production --config webpack.prod.js",
        "start:server": "node dist-server/index.mjs"
    },
    "dependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "aws-sdk": "^2.142.0",
        "basic-auth": "^2.0.0",
        "better-sqlite3": "^12.4.1",
        "body-parser": "^2.2.0",
        "bootstrap-icons": "^1.13.1",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-basic-auth": "^1.1.3",
        "jquery": "^3.5.1",
        "jsdom": "^16.4.0",
        "react-markdown": "^10.1.0",
        "reflect-metadata": "^0.2.2",
        "remark-gfm": "^4.0.1",
        "sqlite3": "^5.1.7",
        "typeorm": "^0.3.27"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "babel-loader": "^8.2.5",
        "bootstrap": "^5.0.0",
        "concurrently": "^9.1.2",
        "copy-webpack-plugin": "^13.0.0",
        "css-loader": "^6.7.1",
        "dry-ux": "^1.76.0",
        "html-webpack-plugin": "^5.5.1",
        "notyf": "^3.10.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.89.2",
        "sass-loader": "^16.0.5",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.1",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-node": "^2.1.9",
        "vite-plugin-dts": "^4.5.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-manifest-plugin": "^5.0.0"
    },
    "author": "Naved Rangwala",
    "license": "ISC"
}
